<Defs>
  <ThingDef ParentName="AnimalThingBase">
    <defName>Shroomimal</defName>
    <label>Shroomimal</label>
    <description>Wild mushroom animals. Got friends with the Fungis thousands of years ago. They've got a devastating Stinger Tail, which can inflict flesh eating spores.</description>
    <statBases>
      <MoveSpeed>6</MoveSpeed>
      <ComfyTemperatureMin>-25</ComfyTemperatureMin>
      <MarketValue>550</MarketValue>
    </statBases>
    <tools>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>12</power>
        <hediff>Fungi_hediff_Spores</hediff>
        <cooldownTime>2.5</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.25</chanceFactor>
      </li>
      <li>
        <label>front left paw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>15</power>
        <hediff>Fungi_hediff_Spores</hediff>
        <cooldownTime>3</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
      </li>
      <li>
        <label>front right paw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>15</power>
        <hediff>Fungi_hediff_Spores</hediff>
        <cooldownTime>3</cooldownTime>
        <linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
      </li>
      <li>
        <label>Tail claw</label>
        <capacities>
          <li>FungiSting</li>
        </capacities>
        <power>50</power>
        <hediff>Fungi_hediff_Spores</hediff>
        <cooldownTime>7</cooldownTime>
        <linkedBodyPartsGroup>FungiStingerAttackTool</linkedBodyPartsGroup>
        <chanceFactor>0.5</chanceFactor>
      </li>
    </tools>
    <comps>
    </comps>
